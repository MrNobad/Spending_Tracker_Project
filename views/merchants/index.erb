<h1>Merchants</h1>

<p>
  <a href="/merchants/new">New Merchant</a>
</p>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th>Amount</th>
    <th></th>
  </tr>
  <% for merchant in @merchants %>
  <tr>
    <td><a <%= merchant.id %>"><%= merchant.name %> </a></td>
    <td></td>
    <td></td>
    <td><form action="/merchants/<%= merchant.id %>/delete" method="POST">
      <input type="submit" value="DELETE">
    </form></td>
  </tr>
  <% end %>
</table>
